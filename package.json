{
  "name": "creators-portal-backend",
  "version": "1.0.0",
  "description": "Backend API for Luna Creators Portal",
  "main": "./src/bin/www/index.ts",
  "private": true,
  "scripts": {
    "dev": "ts-node ./src/bin/www/index.ts",
    "prod:start": "NODE_ENV=production node ./dist/src/bin/www/index.js",
    "dev:watch": "nodemon ./src/bin/www/index.ts",
    "build": "tsc",
    "test": "jest ./src/test/* --coverage",
    "test:watch": "jest --watchAll --collectCoverage=false",
    "test:vulnerability": "npm audit",
    "lint": "eslint './src/**/*.ts' --max-warnings 0",
    "clean": "rm -rf coverage dist",
    "seed": "knex seed:run",
    "knex:migrate:make": "knex --knexfile ./knexfile.ts migrate:make -x ts",
    "knex:migrate:latest": "knex migrate:latest --knexfile knexfile.ts",
    "knex:migrate:rollback": "knex migrate:rollback --knexfile knexfile.tsk"
  },
  "engines": {
    "node": ">=12.10.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Luna-Exchange/CreatorsPortalBackend.git"
  },
  "keywords": [
    "NodeJs API",
    "ExpressJs",
    "Typescript"
  ],
  "author": {
    "name": "Lekky71",
    "email": "lekky@lunamarket.io"
  },
  "license": "LicenseRef-LICENSE",
  "bugs": {
    "url": "https://github.com/Luna-Exchange/CreatorsPortalBackend.git/issues"
  },
  "homepage": "https://github.com/Luna-Exchange/CreatorsPortalBackend.git#readme",
  "dependencies": {
    "@metamask/eth-sig-util": "4.0.1",
    "@types/swagger-ui-express": "4.1.3",
    "aws-sdk": "2.1144.0",
    "axios": "0.27.0",
    "body-parser": "1.19.2",
    "compression": "1.7.4",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "cryptr": "6.0.3",
    "defender-relay-client": "1.23.0",
    "dotenv": "16.0.0",
    "errorhandler": "1.5.1",
    "ethereumjs-util": "7.1.4",
    "express": "4.17.3",
    "express-openapi-validator": "4.13.8",
    "express-validator": "6.14.0",
    "helmet": "5.0.2",
    "http-status-codes": "2.2.0",
    "jsonwebtoken": "8.5.1",
    "knex": "1.0.3",
    "log4js": "6.4.2",
    "method-override": "3.0.0",
    "moralis": "1.5.9",
    "morgan": "1.10.0",
    "pg": "8.7.3",
    "randomstring": "1.2.2",
    "sinon": "13.0.1",
    "swagger-jsdoc": "6.2.0",
    "swagger-ui-express": "4.3.0",
    "typescript": "4.6.2",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.2",
    "@types/compression": "1.7.2",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.12",
    "@types/cryptr": "4.0.1",
    "@types/errorhandler": "1.5.0",
    "@types/express": "4.17.13",
    "@types/jest": "27.4.1",
    "@types/jsonwebtoken": "8.5.8",
    "@types/method-override": "0.0.32",
    "@types/morgan": "1.9.3",
    "@types/node": "17.0.21",
    "@types/randomstring": "1.1.8",
    "@types/sinon": "10.0.11",
    "@types/supertest": "2.0.11",
    "@types/swagger-jsdoc": "6.0.1",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.14.0",
    "@typescript-eslint/parser": "5.14.0",
    "eslint": "8.10.0",
    "jest": "27.5.1",
    "nodemon": "2.0.15",
    "supertest": "4.0.2",
    "ts-jest": "27.1.3",
    "ts-loader": "9.2.7",
    "ts-node": "10.7.0",
    "typescript": "4.6.2",
    "webpack": "5.70.0",
    "webpack-cli": "4.9.2",
    "webpack-node-externals": "3.0.0"
  }
}
